{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "*": [
        "screens/*",
        "navigation/*",
        "components/*",
        "Redux/*",
        "Redux/Slice/*"
      ],
      "tests": [
        "tests/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@Redux/*": [
        "Redux/*"
      ],
      "@Redux/Splice/*": [
        "Redux/Slice/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
